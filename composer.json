{
    "name": "ramonpego/laravel-local",
    "description": "Execute artisan commands on local machine with different users based on spatie/laravel-remote",
    "keywords": [
        "ramonpego",
        "laravel-local"
    ],
    "homepage": "https://github.com/ramonpego/laravel-local",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon PÃªgo",
            "email": "ramon.pego.stl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^7.0",
        "ramonpego/cli": "0.5",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "RamonPego\\Local\\": "src",
            "RamonPego\\Local\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RamonPego\\Local\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RamonPego\\Local\\CommandServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
